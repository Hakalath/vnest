{
  "expo": {
    "name": "Finnish VNeST app",
    "slug": "Finnish VNeST app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "demo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "publicPath": "./",
      "router": {
        "basename": "/vnest"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "be6ea2d4-e636-4bea-b85a-30a3650d101f"
      }
    }
  }
}
